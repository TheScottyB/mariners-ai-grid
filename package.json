{
  "name": "mariners-ai-grid",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild:ios": "expo prebuild --clean --platform ios",
    "prebuild:android": "expo prebuild --clean --platform android",
    "clean": "rm -rf android ios node_modules && pnpm install",
    "nuke": "rm -rf android ios node_modules .expo ~/Library/Developer/Xcode/DerivedData/MarinersAIGrid-* && watchman watch-del-all && pnpm install && npx expo prebuild --clean",
    "test": "vitest run",
    "test:ui": "vitest run src/components",
    "test:logic": "vitest run src/services",
    "doctor": "npx expo-doctor"
  },
  "op-sqlite": {
    "sqliteVec": true,
    "performanceMode": true
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.10.2",
    "@expo/metro-config": "~54.0.12",
    "@gorhom/bottom-sheet": "^5.2.8",
    "@op-engineering/op-sqlite": "^11.0.0",
    "@rnmapbox/maps": "^10.2.10",
    "@turf/bbox": "^7.3.2",
    "@types/ws": "^8.18.1",
    "apache-arrow": "^21.1.0",
    "buffer": "^6.0.3",
    "dotenv": "^17.2.3",
    "expo": "~54.0.31",
    "expo-background-fetch": "^14.0.9",
    "expo-background-task": "^1.0.10",
    "expo-battery": "~10.0.8",
    "expo-blur": "^15.0.8",
    "expo-build-properties": "~1.0.10",
    "expo-constants": "~18.0.13",
    "expo-crypto": "~15.0.8",
    "expo-dev-client": "~6.0.20",
    "expo-device": "~8.0.10",
    "expo-file-system": "~19.0.21",
    "expo-haptics": "~15.0.8",
    "expo-location": "~19.0.8",
    "expo-network": "~8.0.8",
    "expo-notifications": "~0.32.16",
    "expo-secure-store": "~15.0.8",
    "expo-sensors": "~15.0.8",
    "expo-status-bar": "~3.0.9",
    "expo-task-manager": "~14.0.9",
    "expo-updates": "^29.0.16",
    "fzstd": "^0.1.1",
    "nativewind": "^4.2.1",
    "onnxruntime-react-native": "^1.23.2",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "^5.6.2",
    "reconnecting-websocket": "^4.4.0",
    "tailwindcss": "^3.4.19",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@testing-library/react-native": "^13.3.3",
    "@types/react": "~19.1.0",
    "@vitejs/plugin-react": "^5.1.2",
    "babel-preset-expo": "^54.0.9",
    "eas-build-cache-provider": "^16.4.2",
    "jsdom": "^27.4.0",
    "protobufjs": "^8.0.0",
    "react-test-renderer": "^19.1.0",
    "ts-proto": "^2.11.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.17"
  },
  "private": true,
  "packageManager": "pnpm@10.28.0"
}
